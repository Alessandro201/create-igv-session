{
  "exclude": [
    ".*\\.seacr\\..*",
    ".*\\.macs2\\.peaks\\..*",
    ".*\\_summits\\..*",
    ".*\\.frags\\..*",
    ".*/igv/.*",
    ".*\\.consensus\\.peak_counts\\..*"
  ],
  "filetypes_allowed": [
    ".bed",
    ".bed.gz",
    ".bigWig",
    ".bigWig.gz",
    ".gtf",
    ".gtf.gz"
  ],
  "track_features": {
    ".*\\.bed(\\.gz)?$": {
      "height": 25
    },
    ".*_BULK.*\\.consensus\\..*\\.bed(\\.gz)?$": {
      "color": "#018448"
    },
    ".*_TM4.*\\.consensus\\..*\\.bed(\\.gz)?$": {
      "color": "#008cff"
    },
    ".*_BULK_.*\\.bigWig(\\.gz)?$": {
      "color": "rgb(58, 58, 58)"
    },
    ".*_TM4_.*\\.bigWig(\\.gz)?$": {},
    ".*_BULK_.*\\.bed(\\.gz)?$": {},
    ".*_TM4_.*\\.bed(\\.gz)?$": {}
  },
  "group_by_regex": "^([^.]*)_.*\\..*$",
  "set_track_order_within_group": [
    [
      ".*\\.consensus\\..*$"
    ],
    [
      ".*_BULK.*$",
      ".*_TM4.*$"
    ],
    [
      ".*_R(\\d+)\\..*$"
    ],
    [
      ".*\\.bigWig(?:\\.gz)?$",
      ".*\\.bed(?:\\.gz)?$"
    ]
  ]
}
